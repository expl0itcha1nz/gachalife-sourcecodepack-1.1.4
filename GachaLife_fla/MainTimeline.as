         }
      }
      
      public function loopx(e:Event) : void
      {
         if(this.mapx == 3)
         {
            this.circleb.rotation = this.circleb.rotation + 0.3;
            if(this.exitmap == 2)
            {
               if(this.firsttime == 1)
               {
                  this.firsttime = 2;
                  this.savescene();
               }
               this.char.char.gotoAndStop(1);
               this.musicplay = 2;
               this.fademe.gotoAndPlay(2);
               this.exitmap = 1;
               gotoAndStop("home");
               stage.removeEventListener(Event.ENTER_FRAME,this.loopx);
            }
