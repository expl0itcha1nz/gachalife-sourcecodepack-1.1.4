      public function startgacha() : void
      {
         var i:* = undefined;
         if(this.gachapick == 1)
         {
            this.o1 = this.o1 - 5;
            this.totalpulls = this.totalpulls + 1;
            this.gachago.gotoAndStop(2);
            this.rndrate = Math.floor(Math.random() * 100) + 1;
            if(this.rndrate >= 1 && this.rndrate <= 28)
            {
               this.rnditem = Math.floor(Math.random() * 20) + 1;
               this.enterrate = 1;
            }
            else if(this.rndrate >= 29 && this.rndrate <= 53)
            {
               this.rnditem = Math.floor(Math.random() * 20) + 21;
               this.enterrate = 2;
            }
            else if(this.rndrate >= 54 && this.rndrate <= 75)
            {
               this.rnditem = Math.floor(Math.random() * 20) + 41;
               this.enterrate = 3;
            }
            else if(this.rndrate >= 76 && this.rndrate <= 90)
            {
               this.rnditem = Math.floor(Math.random() * 20) + 61;
