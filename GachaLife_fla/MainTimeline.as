            this.char.scaleX = -this.scaleme;
         }
         if(this.lifespawn == 3)
         {
            this.char.x = this.spawn3.x;
         }
         if(this.lifespawn == 4)
         {
            this.char.x = this.spawn4.x;
         }
         if(this.lifespawn == 5)
         {
            this.char.x = this.spawn5.x;
         }
         this.updatequestions();
         this.hideportals();
         if(this.npcchar1 >= 1)
         {
            this.npcload = 1;
            this.presetgo();
         }
         else if(this.npcchar2 >= 1)
         {
            this.npcload = 2;
            this.presetgo();
         }
         else
         {
            this.updatenpcs();
         }
         this.changemusic();
      }
      
