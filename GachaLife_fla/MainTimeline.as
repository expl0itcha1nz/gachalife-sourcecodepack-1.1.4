      public function selectbtx(evt:MouseEvent) : void
      {
         this.firstpick = 0;
         this.secondpick = 0;
         this.leftbar.switchx.visible = false;
         if(this.selecton == 1)
         {
            this.leftbar.slotson.visible = false;
            this.selecton = 2;
         }
         else
         {
            this.leftbar.slotson.visible = true;
            this.selecton = 1;
         }
         this.leftbar.selectbt.gotoAndStop(this.selecton);
      }
      
      public function slotbt1x(evt:MouseEvent) : void
      {
         if(this.mutesfx == 1)
         {
            this.myChannel2 = this.clicksfx.play(0,1);
         }
         if(this.selecton == 1)
         {
            this.slot = 1;
            this.oldpose1 = 0;
            this.leftbar.slotson.gotoAndStop(this.slot);
            this.leftbar.slotson.visible = true;
            this.updatechar();
         }
         else if(this.firstpick == 0)
         {
            this.firstpick = 1;
            this.leftbar.switchx.visible = true;
            this.leftbar.switchx.gotoAndStop(this.firstpick);
         }
         else
         {
            this.secondpick = 1;
            this.leftbar.switchx.visible = false;
            this.switcharoo();
         }
      }
      
