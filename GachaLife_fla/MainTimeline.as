      public function haxdetect() : void
      {
         this.getfid = this.friendid.substr(0,3);
         if(this.fsmall != this.getfid)
         {
            this.hax = 2;
            trace("haxor");
         }
         if(this.o7 <= 500000)
         {
            if(this.o1 >= 200000 || this.o2 >= 25000 || this.levelnum >= 100)
            {
               this.hax = 2;
               trace("haxor");
