      public function onEnterFramex(event:Event) : void
      {
         var xDistance:Number = NaN;
         var yDistance:Number = NaN;
         var angle:Number = NaN;
         if(this.startgame == 2)
         {
            xDistance = clickPoint.x - this.bex.x;
            yDistance = 0;
            angle = Math.atan2(yDistance,xDistance);
            if(angle <= 1.5 && angle >= -1.6)
            {
               this.bex.scaleX = this.scaler;
            }
            else
            {
               this.bex.scaleX = -this.scaler;
            }
            if(Math.abs(xDistance) <= v && Math.abs(yDistance) <= v)
            {
               this.still = 1;
               this.running = 1;
               this.runtimer = 0;
               this.bex.bex.gotoAndPlay("still");
               this.charani = 1;
               removeEventListener(Event.ENTER_FRAME,this.onEnterFramex);
            }
            else
            {
               this.runtimer = this.runtimer - 1;
               this.bex.x = this.bex.x + v * Math.cos(angle);
            }
         }
      }
      
